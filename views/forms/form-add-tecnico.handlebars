{{#each erros}}
    <div class="alert alert-danger">{{texto}}</div>
{{else}}

{{/each}}
<div class="container mt-5">
    <div class="card">
        <div class="card-body">
            <h4>Adiciona nova(o) técnica(o)</h4>
            <form action="/add-tecnico-db" method="POST">
                <div class="form-group mt-5">
                    <label for="tecnico">Técnica(o):</label>
                    <input type="text" name="tecnico" class="form-control" placeholder="Insira a técnica(o)">
                </div>
                <div class="form-group mt-3 align-right">
                    <button type="submit" class="btn btn-success">Salvar</button>
                </div>                
            </form>
        </div>    
    </div>
</div>